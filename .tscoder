// proxy-server.ts
import express from 'express';
import { createProxyMiddleware } from 'http-proxy-middleware';
import vhost from 'vhost';

const app = express();

// Define Minikube IP and service ports
const MINIKUBE_IP = '192.168.49.2';  // Replace with actual Minikube IP
const SERVICE1_PORT = 31239;         // gocd
const SERVICE2_PORT = 30939;         // nginx

console.log(`Minikube IP: ${MINIKUBE_IP}`);

// Proxy middleware for Service 1
const service1Proxy = createProxyMiddleware({
  target: `http://${MINIKUBE_IP}:${SERVICE1_PORT}`,
  changeOrigin: true,
  on: {
    proxyReq: (proxyReq: any, req: any, res: any) => {
      console.log(`Proxying request to Service 1: ${req.method} ${req.url}`);
    },
    proxyRes: (proxyRes, req, res) => {
      console.log(`Received response from Service 1: ${proxyRes.statusCode}`);
    },
  }
});

// Proxy middleware for Service 2
const service2Proxy = createProxyMiddleware({
  target: `http://${MINIKUBE_IP}:${SERVICE2_PORT}`,
  changeOrigin: true,
  on: {
    proxyReq: (proxyReq, req, res) => {
      console.log(`Proxying request to Service 2: ${req.method} ${req.url}`);
    },
    proxyRes: (proxyRes, req, res) => {
      console.log(`Received response from Service 2: ${proxyRes.statusCode}`);
    },
  }
});

// Route traffic based on the host (domain name)
console.log(`traffic http://gocd.istrav.com -> http://${MINIKUBE_IP}:${SERVICE1_PORT}`);
app.use(vhost('gocd.istrav.com', (req, res, next) => {
  console.log('Routing request to gocd.istrav.com');
  service1Proxy(req, res, next);
}));

console.log(`traffic http://nginx.istrav.com -> http://${MINIKUBE_IP}:${SERVICE2_PORT}`);
app.use(vhost('nginx.istrav.com', (req, res, next) => {
  console.log('Routing request to nginx.istrav.com');
  service2Proxy(req, res, next);
}));

// Start the server
const PORT = 8080;
app.listen(PORT, () => {
  console.log(`Reverse proxy running at http://localhost:${PORT}`);
});

// Add a catch-all middleware to log unhandled requests
app.use((req, res) => {
  console.log(`Unhandled request: ${req.method} ${req.url}`);
  res.status(404).send('Not Found');
});

// Error handling middleware
app.use((err, req, res, next) => {
  console.error('Error occurred:', err);
  res.status(500).send('Internal Server Error');
});
</FILE>
